<div id="sc-checkout-prefetch-bottom" data-jsurl="https://images-eu.ssl-images-amazon.com/images/G/02/orderApplication/javascript/pipeline/ocd-no-browserscripts._CB290995146_.js" class="a-row">












<script type="text/javascript">
// send a AJAX call to store the pipedata, retrying after 60 seconds for a max of 5 times
var functionWrap = function (jQuery) {
    var retrypipedata = 5;
    var pipedataattempts = 1;
    var emailchanged = 0;
    var storepipedatahandler;
    var emailchangeattempts = 5;

    function storepipedata() {
        var hframe = jQuery('<iframe src="/gp/buy/prefetch/store-html.html" style="width:0px; height:0px; display:none"></iframe>');
        jQuery('body').append(hframe);

        pipedataattempts++;
        if (pipedataattempts < retrypipedata) {
            storepipedatahandler = setTimeout(storepipedata, 60000);
        }
    }

    jQuery(window).ready(function() {
        if (!emailchanged) {
            storepipedatahandler = setTimeout(storepipedata, 60000);
        }
    });

    jQuery(document).ready(function() {
        jQuery('#ap_email').change(function() {
            if ((emailchanged < emailchangeattempts) && jQuery('#ap_email').val()) {
                jQuery.ajax({url: "/gp/checkoutprefetch/change-email.html", data: {email: jQuery('#ap_email').val()}, headers: {"Cache-Control":"no-cache", "pragma":"no-cache"}});
                if (storepipedatahandler) {
                    clearTimeout(storepipedatahandler);
                }
                emailchanged++;
            }
        });
    });
};

if (typeof window.P === "object" && typeof window.P.when === "function") {
    P.when('jQuery').execute(function($) {
        functionWrap($);
    });
} else if (typeof window.amznJQ === "object") {
    amznJQ.onReady('jQuery', function() {
